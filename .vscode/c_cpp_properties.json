{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/local/opt/llvm/bin/clang++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "compilerArgs": [
                "-pedantic-errors",
                "-fsanitize=undefined",
                "-Wno-deprecated",
                "-Wno-unneeded-internal-declaration"
            ],
            "compileCommands": "${workspaceFolder}/cmake-build-debug/compile_commands.json",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "configurationProvider": "vector-of-bool.cmake-tools"
        }
    ],
    "version": 4
}